emacs --version > /dev/null

if [ $? -eq 0 ] ; then
  alias .e='emacs -nw'
else
  alias .e='nano'
fi

compress() {
  tar cvfz $1.tar.gz $1
}

studio() {
  exec ~/Applications/android-studio/bin/studio.sh &
}

genymotion() {
  exec ~/Applications/genymotion/genymotion &
}

psql() {
 exec "/Applications/Postgres.app/Contents/Versions/9.4/bin/psql" -p5432
}
